#include <iostream>
#include <stack>
#include <string>
using namespace std;

bool checkBalanced(string expression) {
    stack<char> s;
    
    for (int i = 0; i < expression.length(); i++) {
        char ch = expression[i];
        
        if (ch == '(' || ch == '{' || ch == '[') {
            s.push(ch);
        }
        else if (ch == ')' || ch == '}' || ch == ']') {
            if (s.empty()) {
                return false;
            }
            
            char top = s.top();
            s.pop();
            
            if ((ch == ')' && top != '(') ||
                (ch == '}' && top != '{') ||
                (ch == ']' && top != '[')) {
                return false;
            }
        }
    }
    
    return s.empty();
}

int main() {
    string expression;
    cout << "Enter an expression: ";
    getline(cin, expression);
    
    if (checkBalanced(expression)) {
        cout << "Expression has balanced parentheses." << endl;
    } else {
        cout << "Expression does NOT have balanced parentheses." << endl;
    }
    
    return 0;
}
